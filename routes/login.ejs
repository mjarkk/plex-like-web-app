<div class="login-vue">
  <form class="input-from" v-on:submit.prevent="trylogin()">
    <a class="inf" href="https://github.com/mjarkk/plex-like-web-app">Git Repo</a>
    <h1 class="ns">login</h1>
    <div class="input">
      <label class="ns" for="login-username">Username</label>
      <input id="login-username" type="text">
    </div>
    <div class="input">
      <label class="ns" for="login-password">Password</label>
      <input id="login-password" type="password">
    </div>
    <div class="submitbtn">
      <button type="submit" name="button">Log in</button>
    </div>
  </form>
  <div class="background" v-bind:class="{showbg: showbgimg}"></div>
</div>
